{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "outputs": [
        "dist/**"
      ],
      "inputs": [
        "src/**",
        "!src/__tests__"
      ],
      "persistent": true,
      "dependsOn": [
        "^build"
      ]
    },
    "preview": {
      "outputs": [
        "dist/**"
      ],
      "inputs": [
        "src/**",
        "!src/__tests__"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "build": {
      "outputs": [
        "dist/**"
      ],
      "inputs": [
        "src/**",
        "!src/__tests__"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "test": {
      "cache": false,
      "outputs": [
        "coverage/**"
      ],
      "inputs": [
        "src/**/*.{vue, ts}",
        "!src/exports",
        "!src/scripts"
      ]
    },
    "lint": {},
    "lint:fix": {},
    "publish": {
      "dependsOn": [
        "build"
      ]
    }
  }
}